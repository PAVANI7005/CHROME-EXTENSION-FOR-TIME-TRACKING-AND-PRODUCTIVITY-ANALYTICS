{
    "manifest_version": 3,
    "name": "Time Tracker & Productivity Analytics",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "identity",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  